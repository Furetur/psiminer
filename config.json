{
  "filters": [
    {"name": "constructor"},
    {"name": "empty method"},
    {
      "name": "by modifiers",
      "excludeModifiers": ["abstract"]
    }
  ],
  "problem": {"name": "method name prediction"},
  "storage": {"name": "json typed tree"},

  "languages": ["Java"],
  "ignore rules": [
    {"name": "whitespace"},
